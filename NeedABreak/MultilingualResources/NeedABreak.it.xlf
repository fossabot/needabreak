          <target state="needs-review-translation" state-qualifier="exact-match">Chiudi</target>
        </trans-unit>
        <trans-unit id="il_est_l_heure" translate="yes" xml:space="preserve">
          <source>Il est l'heure de bouger !</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">È ora di muoversi!</target>
        </trans-unit>
